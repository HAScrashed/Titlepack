<script><!--
#Include "TextLib" as TL
declare Frame_Settings <=> Page.GetFirstChild("framesettings");
declare Button_Settings <=> Page.GetFirstChild("settings_toggle");
while(True) {
foreach(Event in PendingEvents) {
if (Event.Type == CMlEvent::Type::KeyPress) {
if (Event.CharPressed == "5177344") {
SendCustomEvent("PagerNext", [""]);
} else if (Event.CharPressed == "6815744") {
SendCustomEvent("PagerPrev", [""]);
} else if (Event.CharPressed == "3670016") {
SendCustomEvent("PagerFind", [""]);
}
} else if (Event.Type == CMlEvent::Type::MouseClick) {
if (Event.ControlId == "pager_buttonprev") {
SendCustomEvent("PagerPrev", [""]);
} else if (Event.ControlId == "pager_buttonnext") {
SendCustomEvent("PagerNext", [""]);
} else if (Event.ControlId == "pager_findplayer") {
SendCustomEvent("PagerFind", [""]);
} else if (Event.ControlId == "columns_quadprofile"  && Event.Control.DataAttributeExists("playerlogin")) {
SendCustomEvent("ColumnProfile", [Event.Control.DataAttributeGet("playerlogin")]);
} else if (Event.ControlId == "columns_quadspec" && Event.Control.DataAttributeExists("playerlogin")) {
SendCustomEvent("ColumnSpec", [Event.Control.DataAttributeGet("playerlogin")]);
} else if (Event.ControlId == "settings_toggle") {
if (Frame_Settings.RelativeScale == 0) {
AnimMgr.Add(Frame_Settings, """<frame scale="1" hidden="0" />""", Now, 250, CGameManialinkAnimManager::EAnimManagerEasing::ExpOut);
AnimMgr.Add(Button_Settings, """<quad rot="-110" />""", Now, 250, CGameManialinkAnimManager::EAnimManagerEasing::ExpOut);
} else {
AnimMgr.Add(Frame_Settings, """<frame scale="0" hidden="1" />""", Now, 250, CGameManialinkAnimManager::EAnimManagerEasing::ExpOut);
AnimMgr.Add(Button_Settings, """<quad rot="0" />""", Now, 250, CGameManialinkAnimManager::EAnimManagerEasing::ExpOut);
}
} else if (Event.ControlId == "settings_spectators") {
SendCustomEvent("SettingsSpectators", [""]);
} else if (Event.ControlId == "settings_disconnected") {
SendCustomEvent("SettingsDisconnected", [""]);
} else if (Event.ControlId == "specconfirm_yes") {
SendCustomEvent("SpecConfirmYes", [""]);
} else if (Event.ControlId == "specconfirm_no") {
SendCustomEvent("SpecConfirmNo", [""]);
}
} }yield;}
--></script></manialink>